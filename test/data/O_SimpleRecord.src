# Typecheck of records, simple example of use of records
type recordType = record of {
                    x: int,
                    y: int
                  };
var p : recordType;
var k : array of int;

allocate p;
allocate k of length 10;

k[0] = 3;
p.x = 1;
p.y = 2;
write p.x;
write p.y;
write p.x + p.y;
